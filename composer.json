{
    "name": "djinorm/djin-repo-sql",
    "description": "Djin PDO SQL repository skeleton",
    "keywords": [
        "djin",
        "repository",
        "sql"
    ],
    "authors": [
        {
            "name": "Timur Kasumov aka XAKEPEHOK",
            "role": "Main developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "djinorm/djin": "5.xz-dev",
        "djinorm/djin-filter-sort-paginate": "^1.1",
        "aura/sql": "^2.5",
        "aura/sqlquery": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "phpunit/dbunit": "^4.0"
    },
    "autoload": {
        "psr-4": {"DjinORM\\Repositories\\Sql\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"DjinORM\\Repositories\\Sql\\": "tests"}
    },
    "type": "library",
    "license": "BSD-3-Clause"
}
