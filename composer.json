{
    "name": "djinorm/djin-repo-sql",
    "description": "Djin PDO SQL repository skeleton",
    "keywords": [
        "djin",
        "repository",
        "sql"
    ],
    "authors": [
        {
            "name": "Timur Kasumov aka XAKEPEHOK",
            "role": "Main developer"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../djin",
            "options": {
                "symlink": false
            }
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-PDO": "*",
        "ext-json": "*",
        "djinorm/djin": "dev-ref2",
        "aura/sql": "^2.5",
        "aura/sqlquery": "^2.7",
        "adbario/php-dot-notation": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "phpunit/dbunit": "^4.0"
    },
    "autoload": {
        "psr-4": {"DjinORM\\Repositories\\Sql\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"DjinORM\\Repositories\\Sql\\": "tests"}
    },
    "type": "library",
    "license": "BSD-3-Clause"
}
